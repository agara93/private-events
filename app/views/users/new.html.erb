<h1>Sign Up</h1>
<div class = "row">
    <div class = "col-md-6 col-md-offset-3">
        <%= form_for(@user) do |f| %>
            <%= f.text_field :name %>
            <%= f.text_field :email %>
        
                <%= f.submit "Create my account", class: "btn btn-large btn-primary" %>
  
        <% end %>
    </div>
</div>